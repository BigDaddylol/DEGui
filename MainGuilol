local function scrnshtnft()
	local categ9 = Main:CreateCategory("SNFTS")
	local sect10 = categ9:CreateSection("Main Hacks")

	local nftsmfarmToggle = false

	local nftaf = sect10:Create("Toggle", "Game Win AutoFarm",
		function(state)
			nftsmfarmToggle = state
			if nftsmfarmToggle then
				repeat
					if game:GetService("Workspace").AI:FindFirstChild("Monkey") then
						game:GetService("ReplicatedStorage").Kill:InvokeServer(game:GetService("Workspace").AI.Monkey)
					end
					wait()
				until not nftsmfarmToggle
			end
		end
	)
end

local function udwi()
	local categ10 = Main:CreateCategory("UD:WI")
	local sect11 = categ10:CreateSection("Main Hacks")

	local udfarmToggle = false
	local udbstspd = 10
	local udbsten = false
	local udhndbrk = false
	local udselloc = "Teleports"
	local hmmmmmm = 0
	local udveh = nil

	local function getVeh()
		for i,v in pairs(game.Workspace._Main.Vehicles:GetChildren()) do    
			if v.VehicleSeat.Values.Owner.Value == game.Players.LocalPlayer.Name then
				udveh = v
			end
		end
	end

	--[[
		sect11:Create('Toggle', 'Auto Farm Toggle',
			function(state)
				udfarmToggle = state
				if udfarmToggle then
					udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,-12000)
					repeat
						hmmmmmm = hmmmmmm + 1
						udveh:SetPrimaryPartCFrame(CFrame.new(37673.7227, 57.647377, 28142.0977, 0.478505403, 1.0740731e-07, 0.8780846, -6.94262994e-08, 1, -8.44866719e-08, -0.8780846, -2.05348361e-08, 0.478505403))
						wait(1)
						if hmmmmmm == 6 then
							udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,6000)
						elseif hmmmmmm == 10 then
							udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,-12000)
							hmmmmmm = 0
						end
						print(hmmmmmm)
					until not udfarmToggle
				else
					udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,0)
				end
			end
		)
	]]

	sect11:Create('Toggle', 'Auto Farm Toggle',
			function(state)
				udfarmToggle = state

				getVeh()

				if udfarmToggle then
					repeat
						udveh:SetPrimaryPartCFrame(CFrame.new(37673.7227, 57.647377, 28142.0977, 0.478505403, 1.0740731e-07, 0.8780846, -6.94262994e-08, 1, -8.44866719e-08, -0.8780846, -2.05348361e-08, 0.478505403))
						wait(1)
						keypress(0x57)
					until not udfarmToggle
				else
					udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,0)
				end
			end
		)

	sect11:Create("TextBox", "Boost Speed",
		function(input)
			udbstspd = input
		end,
		{
			text = "Boost Speed"
		}
	)

	sect11:Create("KeyBind", "Boost Toggle", 
		function()
			udbsten = not udbsten

			getVeh()

			if udbsten == true then
				udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,udbstspd/-1*1000)
			elseif udbsten == false then
				udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,0)
			end
		end,
		{
			default = Enum.KeyCode.LeftShift
		}
	)

	sect11:Create("KeyBind", "Hand Brake", 
		function()
			udhndbrk = not udhndbrk

			getVeh()

			if udhndbrk == true then
				udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,100000)
			elseif udhndbrk == false then
				udveh.VehicleSeat.BoostForce.Force = Vector3.new(0,0,0)
			end
		end,
		{
			default = Enum.KeyCode.V
		}
	)

	sect11:Create("DropDown", "Teleports", 
		function(current)
			udselloc = current
		end,
		{
			options = {
				"Westover",
				"Kensington",
				"Palm Shores",
				"South Beach",
				"Woodbury",
				"Nomtauk",
				"Bordenville"
			},
			-- Optional
			default = "Teleports",
			search = true
		}
	)

	sect11:Create("Button", "Teleport To Selected Location",
		function()
			if udselloc == "Teleports" then
				-- Alert notification.
			elseif udselloc == "Westover" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2096.24, 58.9023, 5264.37)
			elseif udselloc == "Kensington" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-4507.25, 9.2061, 891.421)
			elseif udselloc == "Palm Shores" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2576.37, 2.80754, 1700.81)
			elseif udselloc == "South Beach" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3816.79, 6.55009, -9125.41)
			elseif udselloc == "Woodbury" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2524.76, 36.4744, 6575.35)
			elseif udselloc == "Nomtauk" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(37395.3, 50.35, 27565.8)
			elseif udselloc == "Bordenville" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1819.23, 22.0507, -12197.8)
			end
		end,
		{
			animated = true,
		}
	)

	sect11:Create("Button", "Get Rifle",
		function()
			game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer("AddTool", {[1] = "Rifle"})
		end,
		{
			animated = true,
		}
	)
end

local function cdt()
	local categ11 = Main:CreateCategory("CDT")
	local sect12 = categ11:CreateSection("Main Hacks")

	local veh = nil
	local afcount = 0
	local cdtfarmToggle = nil
	local cdtselloc = nil

	sect12:Create("Toggle", "AutoFarm Toggle",
		function(state)
			cdtfarmToggle = state
			if cdtfarmToggle then
				for i, v in pairs(game:GetService("Workspace").Cars:GetChildren()) do
					if v.Stats.Owner.Value == game.Players.LocalPlayer.Name then
						veh = v
					end
				end

				repeat
					if afcount <= 30 then
						keypress(0x57)
						veh.Engine.Velocity = Vector3.new(300,0,0)
						wait(0.2)
						afcount = afcount + 1
					elseif afcount <= 60 then
						keypress(0x53)
						veh.Engine.Velocity = Vector3.new(-300,0,0)
						wait(0.2)
						afcount = afcount + 1
					else
						afcount = 0
						wait(0.2)
					end
				until not cdtfarmToggle
			end
		end
	)
	sect12:Create("Button", "Gift Farm (Spam E)",
		function()
			for i, v in pairs(game.Workspace.Locations:GetChildren()) do
				if v.Active.Value == true then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.SpawnPart.CFrame
					wait(0.4)
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.PlayerGui.BarnGuide.Adornee.CFrame
					wait(0.4)
				end
			end
		end,
		{
			animated = true
		}
	)
	sect12:Create("DropDown", "DropDown", 
		function(current)
			cdtselloc = current
		end,
		{
			options = {
				"AutoFarm Point",
				"Your Dealership"
			},
			-- Optional
			default = "AutoFarm Point",
			search = true
		}
	)
	sect12:Create("Button", "Teleport To Selected Location",
		function()
			if cdtselloc == "AutoFarm Point" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(609.052, 633.249, 310.141)
			elseif cdtselloc == "Your Dealership" then
				for i, v in pairs(game:GetService("Workspace").Tycoons:GetChildren()) do
					if v.Owner.Value == game.Players.LocalPlayer.Name then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Dealership.Spawn.CFrame
					end
				end
			end
		end,
		{
			animated = true,
		}
	)
end

