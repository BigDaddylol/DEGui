		jumplol = js
	end,
	{
		min = 50,
		max = 1000,
		default = 50,
		precise = false,
		changeablevalue = false
	}
)

function onRenderStep()
	if sprinting == false then
		char.Humanoid.WalkSpeed = walklol
	elseif sprinting == true then
		char.Humanoid.WalkSpeed = sprintSpeed
	end
	char.Humanoid.JumpPower = jumplol
end

